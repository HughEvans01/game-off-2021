[{"name":"GOING UP","colour":"0xcc0000","effect":"this.player.jump = this.player.jump*2;"},{"name":"TOO FAST","colour":"0xc1f7f2","effect":"this.player.speed=this.player.speed*2;"},{"name":"BOUNCY","colour":"0xf542f2","effect":"this.player.bounce=1;"},{"name":"Big boy","colour":"0xf5c842","effect":"this.player.sprite.setVelocityY(-this.player.jump);this.player.scale=this.player.scale*2;this.player.sprite.setScale(this.player.scale);"},{"name":"OPPOSITE DAY","colour":"0x83f2e0","effect":"this.player.speed=this.player.speed*-1;"},{"name":"NON-STOP","colour":"0x4287f5","effect":"this.player.idleSpeed=this.player.speed;"},{"name":"SNEAKY","colour":"0x84f542","effect":"this.gameData.enemiesVisible = false;"}]