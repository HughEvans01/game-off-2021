[{"name":"GOING UP","colour":"0x9B3212","effect":"this.player.jump = this.player.jump*2;"},{"name":"TOO FAST","colour":"0xFFFFFF","effect":"this.player.speed=this.player.speed*2;"},{"name":"BOUNCY","colour":"0x565577","effect":"this.player.bounce=1;"},{"name":"Big boy","colour":"0xFFFF33","effect":"this.player.sprite.setVelocityY(-this.player.jump);this.player.scale=this.player.scale*2;this.player.sprite.setScale(this.player.scale);"},{"name":"OPPOSITE DAY","colour":"0x48ECBA","effect":"this.player.speed=this.player.speed*-1;"},{"name":"NON-STOP","colour":"0x3387DD","effect":"this.player.idleSpeed=this.player.speed;"},{"name":"SNEAKY","colour":"0x115522","effect":"this.gameData.enemiesVisible = false;"},{"name":"NO BUGS?","colour":"0xABBBBB","effect":"this.pickups.visible = false;"}]